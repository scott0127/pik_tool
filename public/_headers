# Caching for static assets
/favicon.ico
  Cache-Control: public, max-age=31536000, immutable

/favicon.jpg
  Cache-Control: public, max-age=31536000, immutable

/og-image.png
  Cache-Control: public, max-age=604800

/data/*
  Cache-Control: public, max-age=0, must-revalidate

# Nuxt build assets (hashed)
/_nuxt/*
  Cache-Control: public, max-age=31536000, immutable

/sw.js
  Cache-Control: no-cache
